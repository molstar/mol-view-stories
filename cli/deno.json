{
  "name": "@molstar/mvs-cli",
  "version": "0.1.0",
  "description": "A command-line tool for creating and managing MolViewPack molecular visualization stories",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molstar/mol-view-stories.git"
  },
  "homepage": "https://github.com/molstar/mol-view-stories.git",
  "bugs": {
    "url": "https://github.com/molstar/mol-view-stories/issues"
  },
  "keywords": ["molecular-visualization", "cli", "molstar", "protein", "chemistry", "bioinformatics"],
  "exports": {
    ".": "./mod.ts",
    "./cli": "./main.ts"
  },
  "bin": {
    "mvs": "./main.ts"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "build": "deno compile --allow-read --allow-write --allow-env --allow-net --output mvs main.ts",
    "test": "deno test --allow-read --allow-write --allow-env --allow-run --allow-net --allow-sys",
    "publish": "deno publish"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/yaml": "jsr:@std/yaml@1",
    "@std/fs": "jsr:@std/fs@1",
    "@std/path": "jsr:@std/path@1",
    "@std/cli": "jsr:@std/cli@^1.0.21",
    "@mol-view-stories/lib/": "../@mol-view-stories/lib/src/",
    "@mol-view-stories/lib/StoryManager": "../@mol-view-stories/lib/src/story-manager.ts",
    "@mol-view-stories/lib/types": "../@mol-view-stories/lib/src/types.ts",
    "molstar": "npm:molstar@5.0.0-dev.13",
    "molstar/vec3": "npm:molstar@5.0.0-dev.13/lib/mol-math/linear-algebra/3d/vec3.js",
    "molstar/mvs-data": "npm:molstar@5.0.0-dev.13/lib/extensions/mvs/mvs-data.js",
    "molstar/zip": "npm:molstar@5.0.0-dev.13/lib/mol-util/zip/zip.js",
    "molstar/msgpack-encode": "npm:molstar@5.0.0-dev.13/lib/mol-io/common/msgpack/encode.js",
    "molstar/msgpack-decode": "npm:molstar@5.0.0-dev.13/lib/mol-io/common/msgpack/decode.js",
    "molstar/task": "npm:molstar@5.0.0-dev.13/lib/mol-task/task.js",
    "molstar/lib/extensions/mvs/mvs-data": "npm:molstar@5.0.0-dev.13/lib/extensions/mvs/mvs-data.js",
    "molstar/lib/mol-plugin/version": "npm:molstar@5.0.0-dev.13/lib/mol-plugin/version.js",
    "molstar/lib/mol-util/uuid": "npm:molstar@5.0.0-dev.13/lib/mol-util/uuid.js",
    "molstar/lib/mol-task/index.js": "npm:molstar@5.0.0-dev.13/lib/mol-task/index.js",
    "molstar/lib/mol-io/common/msgpack/encode": "npm:molstar@5.0.0-dev.13/lib/mol-io/common/msgpack/encode.js",
    "molstar/lib/mol-io/common/msgpack/decode": "npm:molstar@5.0.0-dev.13/lib/mol-io/common/msgpack/decode.js",
    "molstar/lib/mol-util/zip/zip": "npm:molstar@5.0.0-dev.13/lib/mol-util/zip/zip.js",
    "molstar/lib/mol-math/linear-algebra": "npm:molstar@5.0.0-dev.13/lib/mol-math/linear-algebra.js",
    "molstar/lib/mol-math/linear-algebra/3d/euler": "npm:molstar@5.0.0-dev.13/lib/mol-math/linear-algebra/3d/euler.js",
    "molstar/lib/mol-util/color/utils": "npm:molstar@5.0.0-dev.13/lib/mol-util/color/utils.js",
    "molstar/lib/mol-task": "npm:molstar@5.0.0-dev.13/lib/mol-task/index.js"
  },
  "compilerOptions": {
    "strict": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "nodeModulesDir": "auto",
  "publish": {
    "exclude": [
      ".git",
      ".gitignore",
      "node_modules",
      "*.log",
      "mvs",
      "src/templates/1fap.pdb",
      "examples/localfile/assets/*.pdb"
    ]
  }
}
