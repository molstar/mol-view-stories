{
  "name": "@molstar/mol-view-stories",
  "version": "1.0.1-dev.1",
  "description": "A TypeScript library for working with Molstar molecular viewer stories",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts"
  },
  "nodeModulesDir": "auto",
  "fmt": {
    "singleQuote": true
  },
  "lint": {
    "exclude": ["tmp/", "node_modules/"]
  },
  "tasks": {
    "dev": "deno test --watch",
    "check": "deno check mod.ts src/*.ts",
    "test": "deno test",
    "publish": "deno publish",
    "mvs-types": "deno run --allow-read --allow-write --allow-env --allow-ffi npm:rollup@4 -c scripts/rollup-mvs-data-bundle.config.mjs && deno run --allow-read --allow-write scripts/process-mvs-types.mjs",
    "build": "deno task mvs-types && deno task check"
  },
  "publish": {
    "exclude": [
      "*.test.ts",
      "*.spec.ts",
      "test/",
      "tests/",
      "tmp/",
      "examples/",
      "node_modules/",
      "scripts/",
      "package.json",
      "tsconfig.json"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "rollup-plugin-dts": "npm:rollup-plugin-dts@^6.2.1",
    "molstar/vec3": "npm:molstar@5.5.0/lib/mol-math/linear-algebra/3d/vec3.js",
    "molstar/mvs-data": "npm:molstar@5.5.0/lib/extensions/mvs/mvs-data.js",
    "molstar/zip": "npm:molstar@5.5.0/lib/mol-util/zip/zip.js",
    "molstar/msgpack-encode": "npm:molstar@5.5.0/lib/mol-io/common/msgpack/encode.js",
    "molstar/msgpack-decode": "npm:molstar@5.5.0/lib/mol-io/common/msgpack/decode.js",
    "molstar/task": "npm:molstar@5.5.0/lib/mol-task/task.js",
    "molstar/lib/mol-task": "npm:molstar@5.5.0/lib/mol-task/index.js",
    "molstar/lib/mol-math/linear-algebra": "npm:molstar@5.5.0/lib/mol-math/linear-algebra.js",
    "molstar/lib/extensions/mvs/mvs-data": "npm:molstar@5.5.0/lib/extensions/mvs/mvs-data.js",
    "molstar/lib/extensions/mvs/mvs-data.js": "npm:molstar@5.5.0/lib/extensions/mvs/mvs-data.js",
    "molstar/lib/mol-plugin/version.js": "npm:molstar@5.5.0/lib/mol-plugin/version.js",
    "molstar/lib/mol-util/uuid.js": "npm:molstar@5.5.0/lib/mol-util/uuid.js",
    "molstar/lib/mol-task/index.js": "npm:molstar@5.5.0/lib/mol-task/index.js",
    "molstar/lib/mol-io/common/msgpack/encode.js": "npm:molstar@5.5.0/lib/mol-io/common/msgpack/encode.js",
    "molstar/lib/mol-io/common/msgpack/decode.js": "npm:molstar@5.5.0/lib/mol-io/common/msgpack/decode.js",
    "molstar/lib/mol-util/zip/zip.js": "npm:molstar@5.5.0/lib/mol-util/zip/zip.js",
    "molstar/lib/mol-math/linear-algebra.js": "npm:molstar@5.5.0/lib/mol-math/linear-algebra.js",
    "molstar/lib/mol-math/linear-algebra/3d/euler.js": "npm:molstar@5.5.0/lib/mol-math/linear-algebra/3d/euler.js",
    "molstar/lib/mol-util/color/utils.js": "npm:molstar@5.5.0/lib/mol-util/color/utils.js",
    "molstar/lib/mol-task.js": "npm:molstar@5.5.0/lib/mol-task.js",
    "molstar/lib/mol-script/language/builder.js": "npm:molstar@5.5.0/lib/mol-script/language/builder.js",
    "molstar/lib/mol-script/language/expression-formatter.js": "npm:molstar@5.5.0/lib/mol-script/language/expression-formatter.js",
    "molstar/lib/mol-util/zip/zip": "npm:molstar@5.5.0/lib/mol-util/zip/zip.js",
    "molstar/lib/mol-util/color/utils": "npm:molstar@5.5.0/lib/mol-util/color/utils.js",
    "molstar/lib/mol-plugin/version": "npm:molstar@5.5.0/lib/mol-plugin/version.js"
  }
}
