{
  "name": "@molstar/mol-view-stories",
  "version": "0.0.1",
  "description": "A TypeScript library for working with Molstar molecular viewer stories",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts"
  },
  "nodeModulesDir": true,
  "tasks": {
    "dev": "deno test --watch",
    "check": "deno check mod.ts src/*.ts",
    "test": "deno test",
    "publish": "deno publish"
  },
  "publish": {
    "exclude": [
      "*.test.ts",
      "*.spec.ts",
      "test/",
      "tests/",
      "examples/",
      "node_modules/",
      "package.json",
      "tsconfig.json"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "molstar/vec3": "npm:molstar/lib/mol-math/linear-algebra/3d/vec3.js",
    "molstar/mvs-data": "npm:molstar/lib/extensions/mvs/mvs-data.js",
    "molstar/zip": "npm:molstar/lib/mol-util/zip/zip.js",
    "molstar/msgpack-encode": "npm:molstar/lib/mol-io/common/msgpack/encode.js",
    "molstar/msgpack-decode": "npm:molstar/lib/mol-io/common/msgpack/decode.js",
    "molstar/task": "npm:molstar/lib/mol-task/task.js",
    "molstar/lib/extensions/mvs/mvs-data": "npm:molstar/lib/extensions/mvs/mvs-data.js",
    "molstar/lib/mol-plugin/version": "npm:molstar/lib/mol-plugin/version.js",
    "molstar/lib/mol-util/uuid": "npm:molstar/lib/mol-util/uuid.js",
    "molstar/lib/mol-task/index": "npm:molstar/lib/mol-task/index.js",
    "molstar/lib/mol-io/common/msgpack/encode": "npm:molstar/lib/mol-io/common/msgpack/encode.js",
    "molstar/lib/mol-io/common/msgpack/decode": "npm:molstar/lib/mol-io/common/msgpack/decode.js",
    "molstar/lib/mol-util/zip/zip": "npm:molstar/lib/mol-util/zip/zip.js",
    "molstar/lib/mol-math/linear-algebra": "npm:molstar/lib/mol-math/linear-algebra.js",
    "molstar/lib/mol-math/linear-algebra/3d/euler": "npm:molstar/lib/mol-math/linear-algebra/3d/euler.js",
    "molstar/lib/mol-util/color/utils": "npm:molstar/lib/mol-util/color/utils.js",
    "molstar/lib/mol-task": "npm:molstar/lib/mol-task.js"
  }
}
